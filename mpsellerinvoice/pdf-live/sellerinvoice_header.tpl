<table style="width: 100%;">
<tr>
	<td style="width: 50%;">
		{if $logo_path}
			<img src="{$logo_path}" style="width:{$width_logo}px; height:{$height_logo}px;" />
		{/if}
	</td>
	<td style="width: 50%; text-align: right;">
		<table style="width: 100%;">
			<tr>
				<td style="font-weight: bold; font-size: 14pt; color: #444; width: 100%;">{if isset($header)}{$header|escape:'html':'UTF-8'|upper}{/if}</td>
			</tr>
			<tr>
				<td style="font-size: 14pt; color: #9E9F9E;">{dateFormat date=$date full=0}</td>
			</tr>
			<tr>
				<td style="font-size: 14pt; color: #9E9F9E;">{$seller_invoice_title|escape:'html':'UTF-8'}</td>
			</tr>
		</table>
	</td>
</tr>
</table>
